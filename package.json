{
  "name": "with-antd",
  "displayName": "With antd",
  "version": "0.0.7",
  "description": "A basic Plasmo extension.",
  "author": "lyx",
  "scripts": {
    "dev": "plasmo dev --no-source-maps",
    "build": "plasmo build"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.2.0",
    "antd": "5.0.7",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tesseract.js": "^5.0.2"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.204",
    "@types/node": "18.11.15",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "http://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*.png",
          "scripts/*"
        ],
        "matches": [
          "https://www.baidu.com/*"
        ]
      }
    ]
  }
}
